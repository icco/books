<!doctype html>
<html>
  <head>
    <title>Nat's To Read</title>
  </head>
  <body>
    <h1>Published Graphic Novels To Read</h1>
    <ul>
      <% @books.each do |b| %>
        <li><a href="https://www.goodreads.com/book/show/<%= b.id %>" title="<%= b.title %>"><img src="<%= b.image_url %>" alt="<%= b.title %>"></a></li>
      <% end %>
    </ul>
  </body>
</html>
